@article{Buick2008,
	author = {Buick, R.},
	title = {{When did Oxygenic Photosynthesis Evolve?}},
	volume = {363},
	number = {1504},
	pages = {2731--2743},
	year = {2008},
	doi = {10.1098/rstb.2008.0041},
	publisher = {The Royal Society},
	abstract = {The atmosphere has apparently been oxygenated since the {\textquoteleft}Great Oxidation Event{\textquoteright} ca 2.4 Ga ago, but when the photosynthetic oxygen production began is debatable. However, geological and geochemical evidence from older sedimentary rocks indicates that oxygenic photosynthesis evolved well before this oxygenation event. Fluid-inclusion oils in ca 2.45 Ga sandstones contain hydrocarbon biomarkers evidently sourced from similarly ancient kerogen, preserved without subsequent contamination, and derived from organisms producing and requiring molecular oxygen. Mo and Re abundances and sulphur isotope systematics of slightly older (2.5 Ga) kerogenous shales record a transient pulse of atmospheric oxygen. As early as ca 2.7 Ga, stromatolites and biomarkers from evaporative lake sediments deficient in exogenous reducing power strongly imply that oxygen-producing cyanobacteria had already evolved. Even at ca 3.2 Ga, thick and widespread kerogenous shales are consistent with aerobic photoautrophic marine plankton, and U{\textendash}Pb data from ca 3.8 Ga metasediments suggest that this metabolism could have arisen by the start of the geological record. Hence, the hypothesis that oxygenic photosynthesis evolved well before the atmosphere became permanently oxygenated seems well supported.},
	issn = {0962-8436},
	URL = {http://rstb.royalsocietypublishing.org/content/363/1504/2731},
	eprint = {http://rstb.royalsocietypublishing.org/content/363/1504/2731.full.pdf},
	journal = {Philos. Trans. R. Soc. Lond. B: Biol. Sci.}
}

